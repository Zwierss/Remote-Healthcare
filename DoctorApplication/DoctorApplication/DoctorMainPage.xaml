<Window x:Class="DoctorApplication.DoctorMainPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DoctorApplication"
        mc:Ignorable="d"
        Title="DoctorMainPage" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock Text="Welkom!" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Button Name="startSessionButton" Grid.Row="1" >Start Session</Button>
        <Button Name="stopSessionButton" Grid.Row="2" >Stop Session</Button>
    </Grid>
</Window>
